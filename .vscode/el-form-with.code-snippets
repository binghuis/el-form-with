      "import { tableBoxDefaultProps, type WETableBoxProps } from \"el-form-with\";",
      "",
      "export interface TableBoxProps extends WETableBoxProps<any> {}",
      "",
      "const TableBox = defineComponent<TableBoxProps>(",
      "  props => {",
      "    return () => {",
      "      return (",
      "        <div class=\"bg-bg_color px-4 pt-4\">",
      "          <ElTable",
